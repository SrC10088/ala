generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Task {
  id           Int      @id @default(autoincrement())
  title        String
  originalText String
  startTime    DateTime?
  priority     String   @default("medium")
  status       String   @default("pending")
  createdAt    DateTime @default(now())
}
